<h1>Home#dashboard</h1>
<p>Find me in app/views/home/dashboard.html.erb</p>

<h2> #TheSecret </h2>

<ul>
  <% @cheets.each do |cheet| %>
    <li> <%= cheet.entry %> </li>
  <% end %>
</ul>

<div class="all users">
  <% @users.each do |user| %>
    <li>
      <% if @current_user.following? user %>
        <%= link_to "Stop Following #{user.email}", stop_following_user_path(id: user.id), method: :post %>
      <% else %>
        <%= link_to "Follow #{user.email}", follow_user_path(id: user.id), method: :post %>
      <% end %>
    </li>
  <% end %>
</div>
